{
  "manifest_version": 3,
  "name": "Fraudify Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension to detect and prevent fraud in online transactions.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "./assets/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./assets/content.js"]
    }
  ]
}