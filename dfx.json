{
  "version": 1,
  "canisters": {
    "ai": {
      "type": "custom",
      "package": "ai",
      "candid": "src/ai/ai.did",
      "optimize": "cycles",
      "wasm": "target/wasm32-unknown-unknown/release/ai.wasm",
      "build": "scripts/build.ai.sh",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "wallet": {
      "candid": "src/wallet/wallet.did",
      "wasm": "target/wasm32-unknown-unknown/release/wallet.wasm",
      "package": "wallet",
      "type": "custom",
      "build": "scripts/build.wallet.sh",
      "_mainnet_init_arg": "(variant { Mainnet })",
      "init_arg": "(variant { Local })",
      "dependencies": ["sol_rpc", "evm_rpc"],
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "sol_rpc": {
      "candid": "https://github.com/dfinity/sol-rpc-canister/releases/download/v1.0.0/sol_rpc_canister.did",
      "package": "sol_rpc_canister",
      "specified_id": "tghme-zyaaa-aaaar-qarca-cai",
      "type": "custom",
      "wasm": "https://github.com/dfinity/sol-rpc-canister/releases/download/v1.0.0/sol_rpc_canister.wasm.gz",
      "init_arg": "( record { manageApiKeys = null; overrideProvider = null; logFilter = opt variant { ShowAll }; numSubnetNodes = null; mode = opt variant { Demo }; } )",
      "remote": {
        "id": {
          "ic": "tghme-zyaaa-aaaar-qarca-cai",
          "playground": "tghme-zyaaa-aaaar-qarca-cai"
        }
      }
    },
    "evm_rpc": {
      "type": "custom",
      "candid": "https://github.com/internet-computer-protocol/evm-rpc-canister/releases/latest/download/evm_rpc.did",
      "wasm": "https://github.com/internet-computer-protocol/evm-rpc-canister/releases/latest/download/evm_rpc.wasm.gz",
      "init_arg": "(record { })",
      "specified_id": "7hfb6-caaaa-aaaar-qadga-cai",
      "remote": {
        "id": {
          "ic": "7hfb6-caaaa-aaaar-qadga-cai"
        }
      }
    },
    "backend": {
      "main": "src/backend/main.mo",
      "type": "motoko",
      "dependencies": ["fradium_ledger"]
    },
    "frontend": {
      "source": ["src/frontend/dist"],
      "type": "assets",
      "workspace": "frontend"
    },
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai",
          "playground": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    },
    "chatbot": {
      "dependencies": ["llm"],
      "main": "src/chatbot/chatbot.mo",
      "type": "motoko"
    },
    "llm": {
      "type": "custom",
      "candid": "https://github.com/dfinity/llm/releases/latest/download/llm-canister-ollama.did",
      "wasm": "https://github.com/dfinity/llm/releases/latest/download/llm-canister-ollama.wasm"
    },
    "icp_ledger": {
      "type": "custom",
      "build": "scripts/build.icp_ledger.sh",
      "candid": "target/ic/icp_ledger.did",
      "wasm": "target/ic/icp_ledger.wasm",
      "specified_id": "ryjl3-tyaaa-aaaaa-aaaba-cai",
      "init_arg_file": "target/ic/icp_ledger.args.did",
      "remote": {
        "id": {
          "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
        }
      }
    },
    "icp_index": {
      "dependencies": ["icp_ledger"],
      "type": "custom",
      "build": "scripts/build.icp_index.sh",
      "candid": "target/ic/icp_index.did",
      "wasm": "target/ic/icp_index.wasm",
      "init_arg_file": "target/ic/icp_index.args.did",
      "specified_id": "qhbym-qaaaa-aaaaa-aaafq-cai",
      "remote": {
        "id": {
          "ic": "qhbym-qaaaa-aaaaa-aaafq-cai"
        }
      }
    },
    "fradium_ledger": {
      "specified_id": "sr4wk-4qaaa-aaaae-qfdta-cai",
      "type": "custom",
      "candid": "src/token/fradium_ledger.did",
      "wasm": "src/token/fradium_ledger.wasm.gz",
      "build": "scripts/build.fradium_ledger.sh",
      "init_arg_file": "target/ic/fradium_ledger.args.did"
    },
    "fradium_index": {
      "specified_id": "vjrnc-hiaaa-aaaam-aejza-cai",
      "type": "custom",
      "candid": "src/token/fradium_index.did",
      "wasm": "src/token/fradium_index.wasm.gz",
      "build": "scripts/build.fradium_index.sh",
      "init_arg_file": "target/ic/fradium_index.args.did",
      "dependencies": ["fradium_ledger"]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    },
    "bitcoin": {
      "enabled": true,
      "nodes": ["127.0.0.1:18444"]
    }
  },
  "output_env_file": ".env",
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral",
      "max_ingress_bytes_per_message": 10000000
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  }
}
